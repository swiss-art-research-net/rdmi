<!-- sari Generator Policy file v2.0 -->
<!-- Author: Matteo Lorenzini ETH-GTA -->
<generator_policy>
  <!-- Concept entity -->
  <generator name="ConceptURI" prefix="sari" shorten="yes">
    <pattern>{term}</pattern>
  </generator>
  <generator name="ConceptURI_2step" prefix="sari" shorten="yes">
    <pattern>{term}/{term1}</pattern>
  </generator>
  <generator name="ConceptURI_3step" prefix="sari" shorten="yes">
    <pattern>{term}/{term1}/{term2}</pattern>
  </generator>
  <generator name="ConceptAppellationURI" prefix="sari" shorten="yes">
    <pattern>Appellation/{term}</pattern>
  </generator>
  <!-- Places entity -->
  <generator name="PlaceURI" prefix="sari" shorten="no">
    <pattern>Place/{term}</pattern>
  </generator>
  <generator name="PlaceAppellationURI" prefix="sari" shorten="yes">
    <pattern>Place/Appellation/{term}</pattern>
  </generator>
  <generator name="PlaceAppellationAddressURI" prefix="sari" shorten="yes">
    <pattern>Place/Appellation/{mainaddress}-{postcode}-{country}</pattern>
  </generator>
  <generator name="PlaceAppLatLong" prefix="sari" shorten="yes">
    <pattern>Place/Appellation/{lat}-{long}</pattern>
  </generator>
  <!-- Project -->
  <generator name="ProjectNameURI" prefix="sari" shorten="yes">
    <pattern>Project_Name/{term}</pattern>
  </generator>
  <generator name="Project_AlternativeURI" prefix="sari" shorten="yes">
    <pattern>Alternative_Name/{term}</pattern>
  </generator>
  <generator name="Projected_FunctionURI" prefix="sari" shorten="yes">
    <pattern>Projected_Function/{term}</pattern>
  </generator>
  <generator name="Project_StartURI" prefix="sari" shorten="yes">
    <pattern>Project_Start_Date/{term}</pattern>
  </generator>
  <generator name="Project_EndURI" prefix="sari" shorten="yes">
    <pattern>Project_End_Date/{term}</pattern>
  </generator>
  <generator name="FinalProjectStatusURI" prefix="sari" shorten="yes">
    <pattern>Final_Status/{term}</pattern>
  </generator>
  <generator name="ProjectedPlaceURI" prefix="sari" shorten="yes">
    <pattern>Projected_Place/{term}</pattern>
  </generator>
  <!-- Oeuvre -->
  <generator name="OeuvreURI" prefix="sari" shorten="no">
    <pattern>Oeuvre/{term}</pattern>
  </generator>
  <generator name="OeuvreAppellationURI" prefix="sari" shorten="yes">
    <pattern>Oeuvre_Appellation/{term}</pattern>
  </generator>
  <generator name="Oeuvre_AlternativeURI" prefix="sari" shorten="yes">
    <pattern>Alternative_Name/{term}</pattern>
  </generator>
  <generator name="Oeuvre_FunctionURI" prefix="sari" shorten="yes">
    <pattern>Oeuvre_Function/{term}</pattern>
  </generator>
  <generator name="Oeuvre_StartURI" prefix="sari" shorten="yes">
    <pattern>Oeuvre_Start_Date/{term}</pattern>
  </generator>
  <generator name="Oeuvre_EndURI" prefix="sari" shorten="yes">
    <pattern>Oeuvre_End_Date/{term}</pattern>
  </generator>
  <generator name="FinalOeuvreStatusURI" prefix="sari" shorten="yes">
    <pattern>Final_Status/{term}</pattern>
  </generator>
  <generator name="OeuvreedPlaceURI" prefix="sari" shorten="yes">
    <pattern>Oeuvre_Place/{term}</pattern>
  </generator>
  <!-- Person entity -->
  <generator name="PersonURI" prefix="sari" shorten="yes">
    <pattern>Person/{term}</pattern>
  </generator>
  <generator name="PersonAppellationURI" prefix="sari" shorten="yes">
    <pattern>Person/Appellation/{term}</pattern>
  </generator>
  <!-- MOA -->
  <generator name="MoaURI" prefix="sari" shorten="yes">
    <pattern>MOA/{term}</pattern>
  </generator>
  <!-- MOE -->
  <generator name="MoeURI" prefix="sari" shorten="yes">
    <pattern>MOE/{term}</pattern>
  </generator>
  <!-- Groups entity -->
  <generator name="GroupsURI" prefix="sari" shorten="yes">
    <pattern>Groups/{term}</pattern>
  </generator>
  <!-- Actor entity -->
  <generator name="ActorURI" prefix="sari" shorten="yes">
    <pattern>Actor/{term}</pattern>
  </generator>
  <generator name="ActorTwoNameMergeURI" prefix="sari" shorten="yes">
    <pattern>Actor/{firstName}_{LastName}</pattern>
  </generator>
  <generator name="ActorAppellationURI" prefix="sari" shorten="yes">
    <pattern>Actor/Appellation/{term}</pattern>
  </generator>
  <!-- Crator -->
  <generator name="CreatorURI" prefix="sari" shorten="yes">
    <pattern>Creator/{term}</pattern>
  </generator>
  <generator name="CreatorAppellationURI" prefix="sari" shorten="yes">
    <pattern>Creator/Appellation/{term}</pattern>
  </generator>
  <!-- Participant -->
  <generator name="ParticipantURI" prefix="sari" shorten="yes">
    <pattern>Participant/{term}</pattern>
  </generator>
  <generator name="ParticipantAppellationURI" prefix="sari" shorten="yes">
    <pattern>Participant/Appellation/{term}</pattern>
  </generator>
  <!-- Event entity -->
  <generator name="EventURI" prefix="sari" shorten="yes">
    <pattern>Event/{term}</pattern>
  </generator>
  <generator name="EventAppellationURI" prefix="sari" shorten="yes">
    <pattern>Event/Appellation/{term}</pattern>
  </generator>
  <!-- Time Span -->
  <generator name="TimeSpanStartURI" prefix="sari" shorten="yes">
    <pattern>Start_Date/{term}</pattern>
  </generator>
  <generator name="TimeSpanEndURI" prefix="sari" shorten="yes">
    <pattern>End_Date/{term}</pattern>
  </generator>
  <generator name="TimeSpan" prefix="sari" shorten="yes">
    <pattern>Chronology/{term}</pattern>
  </generator>
  <!-- ID -->
  <generator name="IdURI" prefix="sari" shorten="yes">
    <pattern>Id/{term}</pattern>
  </generator>
  <!-- Title -->
  <generator name="TitleURI" prefix="sari" shorten="no">
    <pattern>Title/{term}</pattern>
  </generator>
  <!-- Type -->
  <generator name="TypeURI" prefix="sari" shorten="yes">
    <pattern>Type/{term}</pattern>
  </generator>
  <!-- Thing entity -->
  <generator name="ThingURI" prefix="sari" shorten="yes">
    <pattern>Thing/{term}</pattern>
  </generator>
  <generator name="ThingAppellationURI" prefix="sari" shorten="yes">
    <pattern>Thing/Appellation/{term}</pattern>
  </generator>
  <generator name="OneLevelCustomURI" prefix="sari" shorten="yes">
    <pattern>{level1}/{term}</pattern>
  </generator>
  <generator name="TwoLevelCustomURI" prefix="sari" shorten="yes">
    <pattern>{level1}/{level2}/{term}</pattern>
  </generator>
  <generator name="OneLevelCustomURIwCount" prefix="sari" shorten="yes">
    <pattern>{level1}/{count}/{term}</pattern>
  </generator>
  <generator name="TwoLevelCustomURIwCount" prefix="sari" shorten="yes">
    <pattern>{level1}/{level2}/{count}/{term}</pattern>
  </generator>
  <generator name="SimpleLabel">
    <pattern>{label}</pattern>
  </generator>
  <generator name="CompositeLabel">
    <pattern>{label} {text}</pattern>
  </generator>
  <generator name="CompositeVerLabel">
    <pattern>{label} Ver {text}</pattern>
  </generator>
  <generator name="LatLong">
    <pattern>({lat} {long})</pattern>
  </generator>
  <generator name="AddressLabel">
    <pattern>{term},{term2},{term3},{term4}</pattern>
  </generator>
  <generator name="GermanDateTime">
    <custom generatorClass="gr.forth.GermanDate">
      <set-arg name="bound" type="constant"/>
      <set-arg name="text"/>
    </custom>
  </generator>
  <generator name="URIorUUID">
    <custom generatorClass="gr.forth.URIorUUID">
      <set-arg name="text"/>
    </custom>
  </generator>
  <generator name="ConcatMultipleTerms">
    <custom generatorClass="gr.forth.ConcatMultipleTerms">
      <set-arg name="prefix" type="constant"/>
      <set-arg name="sameTermsDelim" type="constant"/>
      <set-arg name="diffTermsDelim" type="constant"/>
      <set-arg name="text1"/>
      <set-arg name="text2"/>
      <set-arg name="text3"/>
      <set-arg name="text4"/>
      <set-arg name="text5"/>
    </custom>
  </generator>
</generator_policy>