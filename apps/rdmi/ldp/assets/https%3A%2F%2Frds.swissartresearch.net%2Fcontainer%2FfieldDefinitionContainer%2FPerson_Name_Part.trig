  _:genid-b5a41d48c88949eabd4467140123c7d2-h9u28l <http://spinrdf.org/sp#text> """INSERT { 
  ?subject crm:P106_is_composed_of ?appellation_uri.
  ?appellation_uri a crm:E41_Appellation .
  ?appellation_uri rdfs:label ?value.
  ?appellation_uri crm:P2_has_type ?type_uri.
  ?type_uri a crm:E55_Type.
  ?type_uri rdfs:label \"Name Part\".
} 
WHERE { 
  BIND(IRI(CONCAT(STR($subject),\"/appellation/\", MD5(str($value)))) as ?appellation_uri).  
  BIND(IRI(CONCAT(\"rds:\",\"types/\", MD5(\"Name Part\"))) as ?type_uri). 
} """;
