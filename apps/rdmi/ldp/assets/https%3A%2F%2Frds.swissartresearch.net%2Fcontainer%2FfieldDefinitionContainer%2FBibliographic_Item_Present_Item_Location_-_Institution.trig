  _:genid-cbbdeffc628147af840dfa191099bbac-2fuu72o <http://spinrdf.org/sp#text> """INSERT { 
  ?subject crm:P53_has_former_or_current_location ?place_uri.
  ?place_uri a crm:E53_Place.
  ?place_uri crm:P74i_is_current_or_former_residence_of ?group_uri.
