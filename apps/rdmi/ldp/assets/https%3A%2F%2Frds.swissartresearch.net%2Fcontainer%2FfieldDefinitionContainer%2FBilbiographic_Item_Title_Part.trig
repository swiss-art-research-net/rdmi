  _:genid-905d3e1887d14cc2b29b17018477487b-2e9i4 <http://spinrdf.org/sp#text> """INSERT { 
  ?subject crm:P106_is_composed_of ?part_uri. 
  ?part_uri a crm:E41_Appellation.
  ?part_uri crm:P2_has_type ?part_uri_type.
  ?part_uri_type rdfs:label \"Part of Title\".
  ?part_uri rdfs:label ?value.
  ?part_uri <http://www.cidoc-crm.org/cidoc-crm/P72_has_language> ?language_iri. 
      ?language_iri a <http://www.cidoc-crm.org/cidoc-crm/E56_Language>. 
