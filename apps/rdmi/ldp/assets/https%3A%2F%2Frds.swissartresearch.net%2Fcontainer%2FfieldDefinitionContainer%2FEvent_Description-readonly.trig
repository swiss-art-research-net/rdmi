  _:genid-5537f190659d4ae6967dbaadc68ce9e1-wkzta8 <http://spinrdf.org/sp#text> """SELECT distinct ?value 
WHERE {
  $subject a crm:E5_Event.
  ?subject crm:P129i_is_subject_of ?info_object_uri .
  ?info_object_uri a crm:E73_Information_Object.
  ?info_object_uri rdfs:label ?description_value.
  ?info_object_uri crm:P2_has_type ?descr_type_uri.
  ?descr_type_uri a crm:E55_Type.
  ?descr_type_uri rdfs:label ?descr_type.
  ?info_object_uri crm:P94i_was_created_by ?creation.
  ?creation crm:P14_carried_out_by ?creator_uri.
  ?creator_uri a crm:E39_Actor. 
  ?creator_uri rdfs:label ?creator.
  ?creation crm:P4_has_time-span ?timespan_uri.
  ?timespan_uri a crm:E52_Time-Span.
  ?timespan_uri crm:P82a_begin_of_the_begin ?from. 
  ?timespan_uri crm:P82b_end_of_the_end ?until
  BIND(CONCAT(?description_value , \" (Author: \",?creator,\") \", \" (Type: \",?descr_type,\") \", \" (Date From: \",str(?from),\") \", \" (Date Until: \",str(?until),\") \") as ?value).
      
}""" .
  
