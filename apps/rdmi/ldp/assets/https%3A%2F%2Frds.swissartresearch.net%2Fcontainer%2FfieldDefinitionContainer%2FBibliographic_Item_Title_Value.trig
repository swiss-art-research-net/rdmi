  _:genid-4ed16e3d0b9c447cb5e77edf9959a4d0-8y6a5m <http://spinrdf.org/sp#text> """SELECT distinct ?value ?label
WHERE {
  $subject a frbr:F24_Publication_Expression.
  ?subject crm:P1_is_identified_by ?appellation_iri . 
  ?appellation_iri a crm:E41_Appellation.
  ?appellation_iri crm:P2_has_type ?appellation_type.
  ?appellation_type rdfs:label \"Prefered Title\".
      ?appellation_iri rdfs:label ?title.
     ?appellation_iri crm:P72_has_language ?lang_uri.
  ?lang_uri rdfs:label ?lang.
  BIND(CONCAT(?title,\" (\",?lang,\")\") as ?value)
}""" .
  
  _:genid-4ed16e3d0b9c447cb5e77edf9959a4d0-5tztu <http://spinrdf.org/sp#text> """INSERT { 
  ?subject rdfs:label ?value.
  ?subject crm:P2_has_type ?appellation_type.
  ?appellation_type rdfs:label \"Prefered Title\".
  ?subject <http://www.cidoc-crm.org/cidoc-crm/P72_has_language> ?language_iri. 
      ?language_iri a <http://www.cidoc-crm.org/cidoc-crm/E56_Language>. 
}   
WHERE {
      ?language_iri <http://www.cidoc-crm.org/cidoc-crm/P3_has_note> ?lang.
      BIND(lang(?value) as ?lang). 
      BIND(IRI(CONCAT(STR($subject),\"/type/\", MD5(\"Prefered Title\"))) as ?appellation_type).
}""" .
  
