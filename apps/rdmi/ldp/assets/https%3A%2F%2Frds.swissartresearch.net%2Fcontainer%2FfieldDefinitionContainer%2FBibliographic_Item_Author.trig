  _:genid-9f213eda5a614a82a76ff119e3caafa7-m69d44 <http://spinrdf.org/sp#text> """INSERT { $subject crm:P01_has_domain ?carried_uri.
      ?carried_uri a crm:PC14_Carried_out_by.
      ?carried_uri crm:P02_has_range ?actor_uri.
      ?actor_uri a crm:E39_Actor.
      ?actor_uri rdfs:label ?value. 
}
WHERE {
	BIND(IRI(CONCAT(STR($subject),\"/carried_uri/\")) as ?carried_uri).
      BIND(IRI(CONCAT(STR($subject),\"/actor/\",MD5(\"?value\"))) as ?actor_uri).
}""" .
  
