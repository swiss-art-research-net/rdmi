
<https://rds.swissartresearch.net/container/fieldDefinitionContainer/Bibliographic_Item_Author/context> {
  _:genid-9f213eda5a614a82a76ff119e3caafa7-ptjm0n <http://spinrdf.org/sp#text> """SELECT ?value ?label WHERE {
  ?subject frbr:R24_created ?pub_event. 
  ?pub_event a frbr:F30_Publication_Event. 	
  $pub_event crm:P01_has_domain ?carried_uri.
    ?carried_uri a crm:PC14_Carried_out_by.
    ?carried_uri crm:P02_has_range ?actor_uri.
    ?actor_uri a crm:E39_Actor.
    ?actor_uri rdfs:label ?actor. 
    ?carried_uri crm:P14.1_in_the_role_of ?role_uri.
    ?role_uri a crm:E55_Type.
    ?role_uri rdfs:label ?role.
      FILTER(?role!=\"Publisher\").
      BIND(CONCAT(?actor,\"(\",?role,\")\") as ?value).
      
}""" .
  
  _:genid-9f213eda5a614a82a76ff119e3caafa7-5jg2kg <http://spinrdf.org/sp#text> """DELETE { $subject crm:P01_has_domain ?carried_uri.} WHERE {
  $subject crm:P01_has_domain ?carried_uri.
    ?carried_uri a crm:PC14_Carried_out_by.
    ?carried_uri crm:P02_has_range ?actor_uri.
    ?actor_uri a crm:E39_Actor.
    ?actor_uri rdfs:label ?value. 
    ?carried_uri crm:P14.1_in_the_role_of ?role_uri.
    ?role_uri a crm:E55_Type.
    ?role_uri rdfs:label ?role.
      FILTER(?role!=\"Publisher\").
}""" .
  
