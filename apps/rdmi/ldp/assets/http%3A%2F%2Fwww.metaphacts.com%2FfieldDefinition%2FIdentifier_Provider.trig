	?actor a <http://www.cidoc-crm.org/cidoc-crm/E39_Actor>.
  	?actor rdfs:label ?value.
}""" .
  
  _:genid-b60f745b28db4fe8b3916e15e3854679-xnbraq <http://spinrdf.org/sp#text> """INSERT {
	?subject crm:P140i_was_attributed_by ?attr_assignment.
	?attr_assignment a crm:E13_Attribute_Assignment.
	?attr_assignment crm:P14_carried_out_by ?actor.
	?actor a crm:E39_Actor.
  	?actor rdfs:label ?value.
} WHERE {
      ?person <http://www.ics.forth.gr/isl/CRMdig/L54_is_same-as> ?subject.
      BIND(IRI(CONCAT(STR(?person),\"/identifier_assignment/\",STR(?subject))) AS ?attr_assignment)
      BIND (UUID() AS ?actor)
}""" .
  
