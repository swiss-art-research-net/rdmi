  _:genid-abd44b92f4904ff8a8662c5eb46950ec-nie0el <http://spinrdf.org/sp#text> """INSERT { 
  ?subject crm:P53_has_former_or_current_location ?place_uri.
  ?place_uri a crm:E53_Place.
  ?place_uri crm:P1_is_identified_by ?address_uri.
  ?address_uri a crm:E41_Appellation .
  ?address_uri rds:label ?value.
  ?address_uri crm:P2_has_type ?type_uri.
  ?type_uri a crm:E55_Type.
  ?type_uri rdfs:label \"Address\".
  ?place_uri crm:P2_has_type ?place_type_uri.
  ?place_type_uri a crm:E55_Type.
  ?place_type_uri rdfs:label \"Institutional Location\".
} 
WHERE { 
  BIND(IRI(CONCAT(STR($subject),\"/place/\", MD5(str($subject)))) as ?place_uri).
  BIND(IRI(CONCAT(STR($subject),\"/address/\", MD5(str($value)))) as ?address_uri).  
  BIND(IRI(CONCAT(\"rds:\",\"types/\", MD5(\"Address\"))) as ?type_uri).
  BIND(IRI(CONCAT(\"rds:\",\"types/\", MD5(\"Institutional Location\"))) as ?place_type_uri).
} """ .
  
