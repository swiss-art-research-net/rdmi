  _:genid-ed0d11d9080742d6993c08cfac304ba6-pn9mi <http://spinrdf.org/sp#text> """DELETE {
  ?subject crm:P107i_is_current_or_former_member_of ?group_iri.
  ?group_iri rdfs:label ?value.
  ?group_iri crm:P2_has_type ?type_uri .
} 
WHERE {
  ?subject crm:P107i_is_current_or_former_member_of ?group_iri.
  ?group_iri a crm:E74_Group.
  ?group_iri rdfs:label ?value.
  ?group_iri crm:P2_has_type ?type_uri .
  ?type_uri a crm:E55_Type.
  ?type_uri rdfs:label \"National Group\".
}""" .
  
  _:genid-ed0d11d9080742d6993c08cfac304ba6-o9jrhw <http://spinrdf.org/sp#text> """INSERT { 
  ?subject crm:P107i_is_current_or_former_member_of ?group_iri.
  ?group_iri a crm:E74_Group.
  ?group_iri rdfs:label ?value.
  ?group_iri crm:P2_has_type ?type_uri .
  ?type_uri a crm:E55_Type.
  ?type_uri rdfs:label \"National Group\".
} 
WHERE {
  BIND(IRI(CONCAT(\"rds:\",\"types/\", MD5(\"National Group\"))) as ?type_uri).  
   BIND(IRI(CONCAT(STR(\"http://grp.swissartresearch.net/group/\"),STRAFTER(STR($subject),\"http://grp.swissartresearch.net/person/\"))) as ?group_iri).
}""" .
  
