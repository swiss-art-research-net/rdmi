  _:genid-baf3f23b291244f88b7a164490707f71-vg5y1o <http://spinrdf.org/sp#text> """INSERT { 
  ?subject crm:P98i_was_brought_into_life ?birth_uri.
  ?birth_uri a crm:E67_Birth .
  ?birth_uri crm:P97_from_father ?father_uri.
  ?father_uri a crm:E21_Person .
  ?father_uri rdfs:label ?value.
} 
WHERE { 
  BIND(IRI(CONCAT(STR($subject),\"/birth\")) as ?birth_uri).  
  BIND(IRI(CONCAT(REPLACE(STR(?subject),\"person/[a-zA-Z0-9_-]*\",\"person/\"), MD5(str(?value)))) as ?father_uri). 
} """;
