  _:genid-720aa80f3f224a629e39bb94d0b276f1-sg4dy <http://spinrdf.org/sp#text> """INSERT { 
  ?subject crm:P98i_was_brought_into_life ?birth_uri.
  ?birth_uri a crm:E67_Birth .
  ?birth_uri crm:P96_by_mother ?mother_uri.
  ?mother_uri a crm:E21_Person .
  ?mother_uri rdfs:label ?value.
} 
WHERE { 
  BIND(IRI(CONCAT(STR($subject),\"/birth\")) as ?birth_uri).  
  BIND(IRI(CONCAT(REPLACE(STR(?subject),\"person/[a-zA-Z0-9_-]*\",\"person/\"), MD5(str(?value)))) as ?mother_uri).
} """ .
  
