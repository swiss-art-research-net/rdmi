  _:genid-45f22669df5741dfa0bb42cbec657f26-f2jw67 <http://spinrdf.org/sp#text> """INSERT { 
  ?subject <http://www.cidoc-crm.org/cidoc-crm/P1_is_identified_by> ?appellation_iri. 
  ?appellation_iri a <http://www.cidoc-crm.org/cidoc-crm/E41_Appellation>.
  ?appellation_iri <http://www.cidoc-crm.org/cidoc-crm/P2_has_type> ?pref_name_type.
  ?pref_name_type rdfs:label \"Prefered Title\".
  ?appellation_iri rdfs:label ?value.
  ?appellation_iri <http://www.cidoc-crm.org/cidoc-crm/P72_has_language> ?language_iri. 
      ?language_iri a <http://www.cidoc-crm.org/cidoc-crm/E56_Language>. 
}   
WHERE {BIND(IRI(CONCAT(STR($subject),\"/type/\", MD5(\"Prefered Title\"))) as ?pref_name_type).
      BIND(IRI(CONCAT(STR($subject),\"/appellation/\", MD5(str(?value)))) as ?appellation_iri).
      ?language_iri <http://www.cidoc-crm.org/cidoc-crm/P3_has_note> ?lang.
      BIND(lang(?value) as ?lang)
}""" .
  
