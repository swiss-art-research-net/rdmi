  _:genid-9914821b0051421a8e23b3860f3d34aa-5thefm <http://spinrdf.org/sp#text> """INSERT { 
  ?subject crm:P108i_was_produced_by ?production_uri.
  ?production_uri a crm:E12_Production.
  ?production_uri crm:P32_used_general_technique ?technique_uri.
  ?technique_uri a crm:E55_Type .
  ?technique_uri rds:label ?value.
  ?production_uri crm:P2_has_type ?type_uri.
  ?type_uri a crm:E55_Type.
  ?type_uri rdfs:label \"Construction\". 
} 
WHERE { 
  BIND(IRI(CONCAT(STR($subject),\"/production/\", MD5(str($subject)))) as ?production_uri).  
  BIND(IRI(CONCAT(STR($subject),\"/technique/\", MD5(str($value)))) as ?technique_uri).  
  BIND(IRI(CONCAT(\"rds:\",\"types/\", MD5(\"construction\"))) as ?type_uri). 
} """ .
  
